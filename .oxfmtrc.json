{
  "$schema": "./node_modules/oxfmt/configuration_schema.json",
  "trailingComma": "es5",
  "tabWidth": 2,
  "semi": true,
  "singleQuote": true,
  "printWidth": 100,
  "importOrder": ["<BUILTIN_MODULES>", "^discord.js$", "<THIRD_PARTY_MODULES>", "^[./]"],
